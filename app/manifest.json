{
  "name": "__MSG_appName__",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "homepage_url": "https://github.com/pd4d10/console-importer",
  "icons": {
    "128": "images/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "*://*.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "scripts/importer.js"
  ]
}
